package com.ridebookingapp.userservice;

import org.apache.catalina.User;

import com.ridebookingapp.model.UserDto;
import com.ridebookingapp.repository.UserRepository;

public class UserServiceImpl implements UserService {

	private UserRepository userRepository;
	
	
	
	@Override
	public User save(UserDto userdto) {
		User user = new User(userdto.getEmail(), userdto.getPassword(), userdto.getRole(),userdto.getName(),userdto.getUserid(), userdto.getUsername());
		
		
		return userRepository.save(user);
	}

}
